{
  "expo": {
    "name": "guardian-project",
    "slug": "guardian-project",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/guardian-owl.png",
    "scheme": "guardian-project",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/guardian-owl.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFD7EA"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/guardian-owl.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "assetBundlePatterns": [
      "**/*"
    ]
  }
}
